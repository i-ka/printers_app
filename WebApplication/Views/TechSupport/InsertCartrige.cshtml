@model WebApplication.Data.Models.Printer
@{
    ViewData["Title"] = "InsertCartrige";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-md-6">
        <form asp-action="InsertCartrige">
            <div class="form-group">
                <select class="form-control" asp-for="CartridgeId" asp-items="@(new SelectList(ViewBag.Cartriges, "Id", "InventoryNumber"))"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Вставить картридж" class="btn btn-success">
            </div>
        </form>
    </div>
</div>

