@model WebApplication.Data.Models.Cartridge
@{
    ViewData["Title"] = "SendCartrige";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form asp-action="SendCartrige">
    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="InventoryNumber" />
    <label asp-for="PlaceId"></label>
    <select class="form-control" asp-for="PlaceId" asp-items="@(new SelectList(ViewBag.Places, "Id", "FullName"))"></select>
    <div class="form-group">
        <input class="btn btn-success" type="submit" value="Отправить" />
    </div>
</form>

